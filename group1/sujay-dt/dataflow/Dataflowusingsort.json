{
	"name": "Dataflowusingsort",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Ds_anu_product_source",
						"type": "DatasetReference"
					},
					"name": "Dfproductsource",
					"description": "Adding product source dataset"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Ds_anu_product_sink",
						"type": "DatasetReference"
					},
					"name": "Dfproductsink"
				}
			],
			"transformations": [
				{
					"name": "Dfpsort"
				}
			],
			"scriptLines": [
				"source(output(",
				"          product_id as integer,",
				"          product_name as string,",
				"          category as string,",
				"          price as integer,",
				"          quantity as integer,",
				"          store_location as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Dfproductsource",
				"Dfproductsource sort(desc(quantity, true)) ~> Dfpsort",
				"Dfpsort sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Dfproductsink"
			]
		}
	}
}