{
	"name": "dataflow3",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Ds_anu_product_source",
						"type": "DatasetReference"
					},
					"name": "sourceproducts"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Ds_anu_product_sink",
						"type": "DatasetReference"
					},
					"name": "sinkproduct",
					"rejectedDataLinkedService": {
						"referenceName": "Ls_group1_gen2",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "FilterByPrice",
					"description": "Filtering rows using expression on column 'price'."
				}
			],
			"scriptLines": [
				"source(output(",
				"          product_id as string,",
				"          product_name as string,",
				"          category as string,",
				"          price as string,",
				"          quantity as string,",
				"          store_location as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceproducts",
				"sourceproducts filter(toInteger(price)>=700&&",
				"toInteger(price)<=1300) ~> FilterByPrice",
				"FilterByPrice sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkproduct"
			]
		}
	}
}