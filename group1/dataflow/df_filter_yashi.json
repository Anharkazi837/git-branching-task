{
	"name": "df_filter_yashi",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_products_yashi",
						"type": "DatasetReference"
					},
					"name": "ProductsctsDfinput1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_sink_yashi",
						"type": "DatasetReference"
					},
					"name": "yashifilteroutput"
				}
			],
			"transformations": [
				{
					"name": "filtertransformation"
				}
			],
			"scriptLines": [
				"source(output(",
				"          product_id as string,",
				"          product_name as string,",
				"          category as string,",
				"          price as string,",
				"          quantity as string,",
				"          store_location as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> ProductsctsDfinput1",
				"ProductsctsDfinput1 filter(toInteger(price) > 100 && ",
				"category == 'Smartphone' &&",
				"toInteger(quantity) > 0) ~> filtertransformation",
				"filtertransformation sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> yashifilteroutput"
			]
		}
	}
}