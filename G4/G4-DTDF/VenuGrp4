## creating and inserting Employee_Dimensiongrp4vvvv

CREATE TABLE Employee_Dimensiongrp4vvvv (
    EmployeeID INT PRIMARY KEY,
    EmployeeName VARCHAR(100),
    Position VARCHAR(100),
    Department VARCHAR(100)
);
INSERT INTO Employee_Dimensiongrp4vvvv (EmployeeID, EmployeeName, Position, Department)
VALUES
    (1, 'John Doe', 'Software Engineer', 'IT'),
