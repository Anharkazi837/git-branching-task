				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "SELECT \n    sales_sujay.SaleID,\n    sales_sujay.ProductID,\n    sales_sujay.Quantity,\n\tsales_sujay.SaleDate,\n\tsales_sujay.TotalAmount,\n    CT.SYS_CHANGE_VERSION, \n    SYS_CHANGE_OPERATION \nfrom sales_sujay\n    RIGHT OUTER JOIN \n    CHANGETABLE(\n    CHANGES sales_sujay, \n    @{activity('LookupLastChangeTrackingVersionActivity').output.firstRow.SYS_CHANGE_VERSION}\n    ) AS CT \n    ON \n    sales_sujay.SaleID = CT.SaleID \nwhere \n    CT.SYS_CHANGE_VERSION <= @{activity('LookupCurrentChangeTrackingVersionActivity').output.firstRow.CurrentChangeTrackingVersion}\n",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
