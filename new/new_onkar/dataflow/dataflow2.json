{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_gen2_product_input",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "DS_product_2_input",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_union_onkar",
						"type": "DatasetReference"
					},
					"name": "targetunion"
				}
			],
			"transformations": [
				{
					"name": "dropnull1"
				},
				{
					"name": "dropnull2"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "union1and2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          product_id as string,",
				"          product_name as string,",
				"          category as string,",
				"          price as string,",
				"          quantity as string,",
				"          store_location as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          product_id as string,",
				"          product_name as string,",
				"          category as string,",
				"          price as string,",
				"          quantity as string,",
				"          store_location as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source2",
				"source1 filter(!isNull(product_id) &&",
				"!isNull(product_name) &&",
				"!isNull(category) &&",
				"!isNull(price) &&",
				"!isNull(quantity) &&",
				"!isNull(store_location)) ~> dropnull1",
				"source2 filter(!isNull(product_id) &&",
				"!isNull(product_name) &&",
				"!isNull(category) &&",
				"!isNull(price) &&",
				"!isNull(quantity) &&",
				"!isNull(store_location)) ~> dropnull2",
				"dropnull1 derive(product_name = replace(product_name,'\"','')) ~> derivedColumn1",
				"dropnull2 derive(product_name = replace(product_name,'\"','')) ~> derivedColumn2",
				"derivedColumn1, derivedColumn2 union(byName: true)~> union1and2",
				"union1and2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> targetunion"
			]
		}
	}
}